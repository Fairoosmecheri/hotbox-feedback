<div style="height: 100vh;">
    <h5>Feedbacks</h5>
    
    <div class="table-container">
        <table class="table">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Feedback</th>
                    <th>Quality &amp Taste</th>
                    <th>Service</th>
                    <th>Consistency</th>
                    <th>Packing</th>
                    <th>Order Again</th>
                </tr>
            </thead>
            <tbody>
                {{#each feedbacks}}
                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.phone}}</td>
                    <td>{{this.feedback}}</td>
                    <td>{{this.quality}}</td>
                    <td>{{this.service}}</td>
                    <td>{{this.consistency}}</td>
                    <td>{{this.taste}}</td>
                    <td>{{this.orderAgain}}</td>
                    
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>



